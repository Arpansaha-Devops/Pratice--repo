const obj ={
    name : "arpan",
    skills : ["js","react","html","css","next js","backend"]
}

// Shallow copy 

const shallow = {...obj}
const shallowanother = Object.assign({},obj)
shallowanother.name = "rahul"
shallowanother.skills.push("database")

// console.log(shallowanother)
